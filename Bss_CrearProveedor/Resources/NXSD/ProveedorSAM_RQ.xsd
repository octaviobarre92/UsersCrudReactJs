      <xsd:element name="RAZONSOCIAL" type="xsd:string" minOccurs="0"/>
	  <xsd:element name="NOMBRECOMERCIAL" type="xsd:string" minOccurs="0"/>
	  <xsd:element name="IDIOMA" type="xsd:string" minOccurs="0"/>
      <xsd:element name="PAIS_RESIDENCIA" type="xsd:string" minOccurs="0"/>
	  <xsd:element name="TIPO_IDENTIFICACION" type="xsd:string" minOccurs="0"/>	  
	  <xsd:element name="DIGITOVERIFICADOR" type="xsd:string" minOccurs="0"/>
	  <xsd:element name="REGISTRO_SANITARIO" type="xsd:string" minOccurs="0"/>
	  <xsd:element name="FECHA_REGISTRO" type="xsd:string" minOccurs="0"/>
