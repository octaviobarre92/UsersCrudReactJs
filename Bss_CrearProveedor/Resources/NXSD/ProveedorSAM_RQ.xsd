	  <xsd:element name="ESTATUS" type="xsd:string" minOccurs="0"/>
	  <xsd:element name="FECHALISTANEGRA" type="xsd:string" minOccurs="0"/>
	  <xsd:element name="CODIGOLISTANEGRA" type="xsd:string" minOccurs="0"/>
	  <xsd:element name="EMAILS" type="tns:EMAIL" minOccurs="0" maxOccurs="unbounded"></xsd:element>	
	  <xsd:element name="ESPECIALIDADES" type="tns:ESPECIALIDAD" minOccurs="0" maxOccurs="unbounded"></xsd:element>	
	  <xsd:element name="RED" type="tns:RED" minOccurs="0" maxOccurs="unbounded"></xsd:element>	
	  <xsd:element name="DIRECCIONES" type="tns:DIRECCION" minOccurs="0" maxOccurs="unbounded"></xsd:element>	
    </xsd:sequence>
  </xsd:complexType>	
  <xsd:complexType name="EMAIL">
    <xsd:sequence>
      <xsd:element name="CORREO" type="xsd:string" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="DIRECCION">
    <xsd:sequence>
      <xsd:element name="NOMBRE_PAIS" type="xsd:string" minOccurs="0"/>
	  <xsd:element name="CODIGO_PAIS" type="xsd:string" minOccurs="0"/>
	  <xsd:element name="NOMBRE_ESTADO" type="xsd:string" minOccurs="0"/>
