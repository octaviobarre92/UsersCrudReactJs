else
{process.poliza = ""}
if((process.body.RESPUESTA_APLICAP != null) && (process.body.RESPUESTA_APLICAP != ""))
{process.resp = process.body.RESPUESTA_APLICAP}
else
{process.resp = ""}
if((process.body.CORE != null) && (process.body.CORE != ""))
{
  process.core = process.body.CORE
  if(process.body.CORE=='P')
  {process.coren='PREMIUM'}
  else if(process.body.CORE=='N')
  {process.coren='SAP'}
  else if(process.body.CORE=='I')
  {process.coren='INSIS'}
  else
  {process.coren=''}
}
else
{process.core = ""}]]></con1:script>
