{process.estado = process.body.ESTADO_PAGO}
else
{process.estado = ""}
if((process.body.MONTO != null) && (process.body.MONTO != ""))
{process.monto = process.body.MONTO}
else
{process.monto = ""}
if((process.body.FECHA != null) && (process.body.FECHA != ""))
{process.fecha = process.body.FECHA}
else
