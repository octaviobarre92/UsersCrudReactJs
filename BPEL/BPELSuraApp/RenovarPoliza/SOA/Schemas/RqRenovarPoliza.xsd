                           </xsd:sequence>
                        </xsd:complexType>
                     </xsd:element>
                     <xsd:element name="RENOVADA">
                        <xsd:complexType>
                           <xsd:sequence>
                              <xsd:element name="NUMERO_DE_POLIZA_ANTERIOR" type="xsd:string"/>
                              <xsd:element name="NUMERO_DE_POLIZA_NUEVA" type="xsd:string"/>
                           </xsd:sequence>
                        </xsd:complexType>
                     </xsd:element>
                     <xsd:element name="CORREDOR" maxOccurs="unbounded">
                        <xsd:complexType>
                           <xsd:sequence>
