                     <xsd:element name="SISTEMA_ORIGEN" type="xsd:string"/>
                  </xsd:sequence>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="OBJETO_POLIZA_CORE">
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="POLIZA">
                        <xsd:complexType>
                           <xsd:sequence>
