      </wsdl:output>
      <wsdl:fault name="legalizarRecibosFaultMsg">
        <soap:fault name="legalizarRecibosFaultMsg" use="literal"/>
      </wsdl:fault>
    </wsdl:operation>
  </wsdl:binding>
  <wsdl:service name="expoServiceSOAP">
