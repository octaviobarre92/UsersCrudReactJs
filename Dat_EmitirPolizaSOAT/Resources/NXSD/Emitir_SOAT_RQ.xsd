           <xsd:element name="RELACION" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="OBJETO_ASEGURADO">
    <xsd:sequence>
	<xsd:element name="AUTO" type="tns:AUTO" minOccurs="0" maxOccurs="1"></xsd:element>               
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="AUTO">
    <xsd:sequence>
	   <xsd:element name="TIPO_AUTO" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
           <xsd:element name="SUBTIPO_AUTO" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
           <xsd:element name="NUMERO_PLACA" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
           <xsd:element name="MARCA" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
           <xsd:element name="MODELO" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
           <xsd:element name="NO_MOTOR" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
           <xsd:element name="CHASIS" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
           <xsd:element name="ACTIVIDAD" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
           <xsd:element name="TONELAJE" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
           <xsd:element name="NUMERO_ASIENTOS" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
           <xsd:element name="NUMERO_PUERTAS" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
           <xsd:element name="TIPO_PINTURA" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
           <xsd:element name="ANNIO" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
           <xsd:element name="CANTIDAD_ANNIO" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
           <xsd:element name="COLOR" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
           <xsd:element name="REGISTRO" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
           <xsd:element name="CABALLOS_FUERZA" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
           <xsd:element name="TONELAJE_CARGA" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
           <xsd:element name="TIPO_MOTOR" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
           <xsd:element name="TIPO_COMBUSTIBLE" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
           <xsd:element name="TIPO_CONDUCTOR" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
           <xsd:element name="TIPO_CARROCERIA" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
           <xsd:element name="COLOR_VENTANAS" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
           <xsd:element name="DESCRIPCION" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>           
           <xsd:element name="ACREEDOR" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>           
           <xsd:element name="ZONA_CIRCULACION" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>           
           <xsd:element name="ACCESORIOS" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>           
           <xsd:element name="MONTO_ASEGURADO" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
           <xsd:element name="FORMA_PAGO" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>
    
 <xsd:annotation xmlns="">
    <xsd:appinfo>NXSDSAMPLE=</xsd:appinfo>
    <xsd:appinfo>USEHEADER=false</xsd:appinfo>
  </xsd:annotation>
</xsd:schema>